	<tool displayname="GeoPy Address Lookup" name="AddressLookup" toolboxalias="CustomPythonTools" xmlns="">
		<parameters>
			<param datatype="Feature Class" direction="Input" displayname="Input Feature" expression="inFeature" name="inFeature" type="Required">
				<dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The feature class with coordinates to look up.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference>
			</param>
			<param datatype="Boolean" direction="Input" displayname="Address Field Exists" expression="addressExists" name="addressExists" type="Required">
				<dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Specify if the address field exists in the attribute table.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN&gt;Checked - The address field exists.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN&gt;Unchecked - The address field does not exist. A new field called "Address" will be created. This is the DEFAULT.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN /&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference>
			</param>
			<param datatype="String" direction="Input" displayname="Address Field" expression="{addressField}" name="addressField" type="Optional">
				<dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The field that is used to store address values.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference>
			</param>
			<param datatype="String" direction="Input" displayname="Service" expression="Google | OpenStreetMap" name="service" type="Required">
				<dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The service used to search for addresses.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference>
			</param>
		</parameters>
		<summary>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;This tool uses the &lt;/SPAN&gt;&lt;A href="https://github.com/geopy/geopy"&gt;&lt;SPAN&gt;GeoPy&lt;/SPAN&gt;&lt;/A&gt;&lt;SPAN&gt;Python package in order to conduct a reverse geocode using the various geocoding APIs.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;A href="file://balfil03/civproj/EVAC_MGT/General_References/Tutorials/Get PIP instructions.docx"&gt;&lt;SPAN&gt;Installing GeoPy&lt;/SPAN&gt;&lt;/A&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN STYLE="font-weight:bold;"&gt;Note: If using the Google service, as a free user, you are limited to 2500 queries a day at a rate of 5 queries per second.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</summary>
		<usage>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;This tool uses various geocoding APIs to find a feature's adddress using it its lat/lon coordinates.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN&gt;The address is populated to an existing or newly created field in the Street, City, State, Zip format.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</usage>
		<arcToolboxHelpPath>c:\program files (x86)\arcgis\desktop10.2\Help\gp</arcToolboxHelpPath>
	</tool>
